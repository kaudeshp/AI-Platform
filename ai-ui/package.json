{
  "name": "ai-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "run-s _build-client _build-server _ready-start",
    "start:client": "react-scripts-ts start ",
    "start:server": "node-dev ./dist/server.js --all-deps --dedupe --inspector",
    "build": "run-s _build-client _build-server",
    "eject": "react-scripts-ts eject",
    "test": "react-scripts-ts test --env=jsdom",
    "_copy-client": "xcopy build client /O /X /E /H /K /Y /I",
    "_spawn-service": "start cmd /c \"npm run start:service\"",
    "_ready-start": "run-p _spawn-service _watch-client _watch-server",
    "_build-client": "react-scripts-ts build && npm run _copy-client",
    "_watch-client": "nodemon --exec \"npm run _build-client\" --watch src --watch public -e ts,tsx,css",
    "_watch-server": "tsc --watch",
    "_build-server": "tsc"
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts-ts": "2.15.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.11.1",
    "@types/jest": "^22.2.3",
    "@types/jss": "^9.5.3",
    "@types/mongoose": "^5.0.14",
    "@types/node": "^6.0.110",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-test-renderer": "^16.0.1",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "cpy-cli": "^1.0.1",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "global": "^4.3.2",
    "jss": "^9.8.1",
    "jss-preset-default": "^4.5.0",
    "mongoose": "^5.0.16",
    "node-dev": "^3.1.3",
    "nodemon": "^1.17.4",
    "office-ui-fabric-react": "^5.100.0",
    "prop-types": "^15.6.1",
    "react-jss": "^8.4.0",
    "react-test-renderer": "^16.3.2",
    "typescript": "^2.8.3",
    "yarn-run-all": "^3.1.1"
  }
}
