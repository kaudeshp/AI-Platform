{
  "name": "ai-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "yarn build",
    "clean": "rimraf dist build client build-final",
    "build:final": "yarn clean && yarn build:server && yarn build:client && yarn mkdir:build-final && ncp client build-final/client && webpack",
    "mkdir:build-final": "mkdir -p build-final",
    "docker": "yarn build:final && docker image build -t fuulatlive.azurecr.io/ai2ai/ux:1 .",
    "deploy": "cmd /c \"date /t >tmp/restart.txt\" && cmd /c \"time /t >>tmp/restart.txt\" && git commit -m \"touching for deployment\" ./tmp/restart.txt && git push -v ai2ai2 master",
    "build": "run-s build:client build:server",
    "build:watch": "run-p build:client:watch build:server:watch",
    "build:client": "react-scripts-ts build && yarn copy:client",
    "build:client:watch": "nodemon --exec \"yarn build:client\" --watch src/app --watch public -e ts,tsx,css --ignore dist --ignore build --ignore client",
    "build:server": "tsc",
    "build:server:watch": "tsc --watch",
    "start": "run-s build start:server",
    "start:prod": "set NODE_ENV=production && node build-final/server/index.js",
    "start:watch": "run-p build:watch start:server:watch",
    "start:prod:watch": "set NODE_ENV=production && yarn start:watch",
    "start:client": "react-scripts-ts start ",
    "start:server": "node ./server.js",
    "start:server:watch": "node-dev ./server.js --all-deps --dedupe --inspector",
    "spawn:start:server": "start cmd /c \"yarn start:server\"",
    "spawn:start:server:watch": "start cmd /c \"yarn start:server:watch\"",
    "eject": "react-scripts-ts eject",
    "test": "react-scripts-ts test --env=jsdom",
    "copy:client": "ncp build client"
  },
  "dependencies": {
    "find-up": "^3.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts-ts": "2.15.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.11.1",
    "@types/jest": "^22.2.3",
    "@types/jss": "^9.5.3",
    "@types/mongoose": "^5.0.14",
    "@types/node": "^6.0.110",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-test-renderer": "^16.0.1",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "global": "^4.3.2",
    "jss": "^9.8.1",
    "jss-preset-default": "^4.5.0",
    "mongoose": "^5.0.16",
    "ncp": "^2.0.0",
    "node-dev": "^3.1.3",
    "nodemon": "^1.17.4",
    "office-ui-fabric-react": "^6.61.2",
    "prop-types": "^15.6.1",
    "react-chartkick": "^0.3.0",
    "react-jss": "^8.4.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.3.2",
    "rimraf": "^2.6.3",
    "ts-loader": "^5.3.3",
    "typescript": "^2.8.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "yarn-run-all": "^3.1.1"
  }
}
